#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num;
    scanf("%d",&num);
    if(num<1)
        printf("Wind condition is Calm");
    else if(num>=1 && num<=3)
        printf("Wind condition is Light Air");
    else if(num>=4 && num<=27)
        printf("Wind condition is Breeze");
     else if(num>=28 && num<=47)
        printf("Wind condition is Gale");
     else if(num>=48 && num<=63)
        printf("Wind condition is Storm");
    else
        printf("Wind condition is Hurricane");
    
    return 0;
}
