#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char a;
    scanf("%c",&a);
    if((a=='A' || a=='E' || a=='I' || a=='O' || a=='U') ||  (a=='a' || a=='e' || a=='i' || a=='o' || a=='u'))
        printf("YES");
    else 
        printf("NO");

   
    return 0;
}
